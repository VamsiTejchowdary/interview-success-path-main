{
  "version": 2,
  "builds": [
    {
      "src": "stripe-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/stripe-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
} 